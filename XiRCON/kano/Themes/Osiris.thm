proc flagc {nick chan {c1 ""} {c2 ""}} {
    set ch ""
    if {[isop $nick $chan]} {set ch @} elseif {[isvoice $nick $chan]} {set ch +}
    if {$c1 == ""} {set c1 [10]}
    if {$c2 == ""} {set c2 0}
    return $c1$ch$c2$nick
}

